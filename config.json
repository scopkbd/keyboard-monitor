{  "logging": {
    "data_file": "./data/keyboard_log.json",
    "backup_interval": 3600,
    "max_sequence_length": 1000,
    "auto_save": true,
    "save_interval": 300,
    "idle_save_delay": 1.0,
    "continuous_save_interval": 300,
    "keystroke_batch_save": 100
  },
  "display": {
    "refresh_interval": 1.0,
    "color_output": true,
    "show_realtime": true,
    "max_display_keys": 10
  },
  "privacy": {
    "exclude_passwords": true,
    "anonymize_data": false,
    "exclude_sensitive_apps": []
  },
  "analysis": {
    "track_modifiers": true,
    "track_sequences": true,
    "track_bigrams": true,
    "track_trigrams": true,
    "min_sequence_count": 2
  },
  "system": {
    "log_level": "INFO",
    "max_log_size": 10485760,
    "max_log_files": 5
  }
}